 val data = Seq(("Project","A", 1),
    ("Gutenberg’s", "X",3),
    ("Alice’s", "C",5),
    ("Adventures","B", 1)
  )
  val rdd=spark.sparkContext.parallelize(data)
  rdd.foreach(println)
  val rdd2=rdd.map(f=>{(f._2, (f._1,f._2,f._3))})
  rdd2.foreach(println)
  val rdd3= rdd2.sortByKey()
  rdd2.foreach(println)
}

  
